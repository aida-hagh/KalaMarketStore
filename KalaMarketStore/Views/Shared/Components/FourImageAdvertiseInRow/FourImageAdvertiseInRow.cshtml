@model IEnumerable<ImageAdvertise>



@if (Model != null)
{
    @foreach (var item in Model)
    {
        <div class="col-6 col-lg-3 pr">
            <a href="#" class="adplacement-item">
                <div class="adplacement-sponsored_box">
                    <img src="/Site/assets/images/@item.ImageAdvertise_Image" alt="@item.ImageAdvertise_Title">
                </div>
            </a>
        </div>

    }

}
